package corejava.collections.chat;

public class ChatApplication {

	public static void main(String[] args) {
		
		do{
			System.out.println("\n 1) Create a Chatroom \n 2) Add the User \n 3) User Login \n 4)Send a Message \n 5) Display the Messages from a specific Chatroom \n 6) List down all users belonging to the specified Chatroom \n 7) Logout \n 8) Delete an User \n 9) Exit \n\n Enter your Choice: ");
		}while(true);
	}

}
