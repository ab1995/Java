package corejava.collections.chat;

public class ChatApplication {

	public static void main(String[] args) {
		
		do{
			System.out.println("\n 1) Create a chatroom \n 2) Add the user \n 3) User login \n 4)Send a message \n 5) Display the messages from a specific chatroom \n 6) List down all users belonging to the specified chat room \n 7)");
		}while(true);
	}

}
